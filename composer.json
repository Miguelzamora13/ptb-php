{
    "name": "devdasher/ptb-php",
    "description": "A PHP Procedural library for Telegram Bot API",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "DevDasher\\PTB_PHP\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "src/Config/config.php",
            "src/Handlers/handlers.php",
            "src/Telegram/Helpers/helpers.php",
            "src/Telegram/Methods/methods.php",
            "src/Telegram/Types/types.php"
        ]
    },
    "authors": [
        {
            "name": "Pooria Bashiri",
            "email": "po.pooria@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "symfony/cache": "^6.3",
        "psr/simple-cache": "^3.0",
        "laravel/serializable-closure": "^1.3",
        "devdasher/php-helpers": "^1.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
